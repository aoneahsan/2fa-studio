{
	"name": "2fa-studio",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"setup": "node setup.js",
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"cap:sync": "npx cap sync",
		"cap:android": "npx cap open android",
		"cap:ios": "npx cap open ios",
		"type-check": "tsc --noEmit",
		"clean": "rm -rf dist node_modules .turbo",
		"postinstall": "test -f .env || cp .env.example .env",
		"cap:run:ios": "npx cap run ios",
		"cap:run:android": "npx cap run android",
		"ext:build": "cd chrome-extension && zip -r ../2fa-studio-extension.zip .",
		"ext:watch": "cd chrome-extension && echo 'Load unpacked extension in Chrome developer mode'",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage"
	},
	"dependencies": {
		"@capacitor-community/barcode-scanner": "^4.0.1",
		"@capacitor/android": "^7.4.2",
		"@capacitor/app": "^7.0.1",
		"@capacitor/barcode-scanner": "^2.0.3",
		"@capacitor/camera": "^7.0.1",
		"@capacitor/cli": "^7.4.2",
		"@capacitor/core": "^7.4.2",
		"@capacitor/device": "^7.0.1",
		"@capacitor/filesystem": "^7.1.2",
		"@capacitor/ios": "^7.4.2",
		"@capacitor/network": "^7.0.1",
		"@capacitor/preferences": "^7.0.1",
		"@headlessui/react": "^2.2.4",
		"@heroicons/react": "^2.2.0",
		"@reduxjs/toolkit": "^2.8.2",
		"@types/node": "^24.0.13",
		"buildkit-ui": "^0.0.5",
		"capacitor-auth-manager": "^0.0.2",
		"capacitor-biometric-auth": "^0.1.1",
		"capacitor-firebase-kit": "^0.0.3",
		"capacitor-native-update": "^0.0.3",
		"firebase": "^11.10.0",
		"otpauth": "^9.4.0",
		"qr-scanner": "^1.4.2",
		"qrcode": "^1.5.4",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-redux": "^9.2.0",
		"react-router-dom": "^7.6.3",
		"tailwindcss": "^4.1.11"
	},
	"devDependencies": {
		"@eslint/js": "^10.0.0",
		"@tailwindcss/vite": "^4.1.11",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/gapi": "^0.0.47",
		"@types/gapi.client": "^1.0.8",
		"@types/gapi.client.drive": "^3.0.15",
		"@types/google.accounts": "^0.0.17",
		"@types/qrcode": "^1.5.5",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@typescript-eslint/eslint-plugin": "^8.36.0",
		"@typescript-eslint/parser": "^8.36.0",
		"@vitejs/plugin-react": "^4.6.0",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.31.0",
		"eslint-plugin-react-hooks": "^6.0.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"jsdom": "^26.1.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.36.0",
		"vite": "^7.0.4",
		"vite-plugin-pwa": "^1.0.1",
		"vitest": "^3.2.4"
	}
}
