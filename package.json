{
	"name": "2fa-studio",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"setup": "node setup.js",
		"dev": "vite",
		"build": "tsc --noEmit && vite build",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"lint:quiet": "eslint . --quiet",
		"preview": "vite preview",
		"cap:sync": "npx cap sync",
		"cap:android": "npx cap open android",
		"cap:ios": "npx cap open ios",
		"type-check": "tsc --noEmit --skipLibCheck",
		"clean": "rm -rf dist node_modules .turbo",
		"postinstall": "test -f .env || cp .env.example .env",
		"cap:run:ios": "npx cap run ios",
		"cap:run:android": "npx cap run android",
		"ext:build": "cd chrome-extension && zip -r ../2fa-studio-extension.zip .",
		"ext:watch": "cd chrome-extension && echo 'Load unpacked extension in Chrome developer mode'",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"cypress:open": "cypress open",
		"cypress:run": "cypress run",
		"e2e": "cypress run --e2e",
		"e2e:headed": "cypress run --e2e --headed",
		"deploy": "./scripts/deploy.sh",
		"deploy:hosting": "firebase deploy --only hosting",
		"deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
		"deploy:all": "firebase deploy",
		"docs:start": "cd docs-site && yarn start",
		"docs:build": "cd docs-site && yarn build",
		"docs:serve": "cd docs-site && yarn serve",
		"docs:deploy": "cd docs-site && yarn deploy",
		"docs:clear": "cd docs-site && yarn clear",
		"docs:write-translations": "cd docs-site && yarn write-translations",
		"docs:write-heading-ids": "cd docs-site && yarn write-heading-ids"
	},
	"dependencies": {
		"@capacitor-community/admob": "^7.0.3",
		"@capacitor/android": "^7.4.2",
		"@capacitor/app": "^7.0.2",
		"@capacitor/barcode-scanner": "^2.0.3",
		"@capacitor/browser": "^7.0.2",
		"@capacitor/camera": "^7.0.2",
		"@capacitor/cli": "^7.4.2",
		"@capacitor/clipboard": "^7.0.2",
		"@capacitor/core": "^7.4.2",
		"@capacitor/device": "^7.0.2",
		"@capacitor/dialog": "^7.0.2",
		"@capacitor/filesystem": "^7.1.4",
		"@capacitor/haptics": "^7.0.2",
		"@capacitor/ios": "^7.4.2",
		"@capacitor/network": "^7.0.2",
		"@capacitor/preferences": "^7.0.2",
		"@capacitor/share": "^7.0.2",
		"@capacitor/toast": "^7.0.2",
		"@capawesome/capacitor-file-picker": "^7.2.0",
		"@headlessui/react": "^2.2.7",
		"@heroicons/react": "^2.2.0",
		"@reduxjs/toolkit": "^2.8.2",
		"@stripe/react-stripe-js": "^3.9.0",
		"@stripe/stripe-js": "^7.8.0",
		"@types/crypto-js": "^4.2.2",
		"bcryptjs": "^3.0.2",
		"buffer": "^6.0.3",
		"buildkit-ui": "^1.3.0",
		"capacitor-auth-manager": "^2.1.0",
		"capacitor-biometric-authentication": "^2.0.0",
		"capacitor-firebase-kit": "^2.0.1",
		"capacitor-native-update": "^2.0.0",
		"capacitor-secure-storage-plugin": "^0.11.0",
		"classnames": "^2.5.1",
		"clsx": "^2.1.1",
		"code-craft-studio": "^2.0.0",
		"crypto-js": "^4.2.0",
		"firebase": "^12.1.0",
		"formik": "^2.4.6",
		"framer-motion": "^12.23.12",
		"google-auth-library": "^10.2.1",
		"googleapis": "^155.0.1",
		"lucide-react": "^0.539.0",
		"notification-kit": "^2.0.5",
		"onesignal-cordova-plugin": "^5.2.15",
		"otpauth": "^9.4.1",
		"otplib": "^12.0.1",
		"qr-scanner": "^1.4.2",
		"qrcode": "^1.5.4",
